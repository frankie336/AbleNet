ip vrf vpn00001
 description Managetment
 rd 65001:1000
 route-target export 65001:1000
 route-target import 65001:1000
!
!
interface GigabitEthernet0/0
 description Customer_CE_Trunk
 no ip address
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/0.3
 description vpn00001_Service_management
 encapsulation dot1Q 103
 ip vrf forwarding vpn00001
 ip address 10.0.2.9 255.255.255.252
!
interface GigabitEthernet0/0.901
 encapsulation dot1Q 901
 ip vrf forwarding vpn00001
 ip address 172.16.2.1 255.255.255.252
!
interface GigabitEthernet0/0.902
 encapsulation dot1Q 902
 ip vrf forwarding vpn00001
 ip address 172.16.2.5 255.255.255.252
!
interface Loopback1
 ip vrf forwarding vpn00001
 ip address 172.16.0.7 255.255.255.255
!
router bgp 65000
 !
 address-family ipv4 vrf vpn00001
  redistribute connected
  redistribute static
  neighbor 10.0.2.10 remote-as 65001
  neighbor 10.0.2.10 description vpn0001_service_router
  neighbor 10.0.2.10 password ioLe8/I7)WxH
  neighbor 10.0.2.10 activate
  neighbor 10.0.2.10 as-override
 exit-address-family
!
ip route vrf vpn00001 172.16.0.2 255.255.255.255 172.16.2.2
ip route vrf vpn00001 172.16.0.4 255.255.255.255 172.16.2.6
